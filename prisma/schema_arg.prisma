  RemovedBy  Int?
  RemoveType String? @db.VarChar(3)
  RemovedOn  Int?
  type       Int     @default(0) @db.TinyInt
  ureason    String? @db.Text

  @@index([RemoveType], map: "RemoveType")
  @@index([aid], map: "aid")
  @@index([authid], map: "authid")
  @@index([created], map: "created")
  @@index([sid], map: "sid")
  @@index([type], map: "type")
}

